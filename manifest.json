{
    "name": "Glassmorphism",
    "version": "23.3.14",
    "manifest_version": 3,
    "author": "Truong Giang",
    "description": "Chrome Glassmorphism Theme",
    "background": { "service_worker": "./assets/js/background.js" },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["webRequest", "activeTab", "tabs", "unlimitedStorage", "storage", "background", "notifications"],
    "icons": {
        "16": "./assets/images/browser.png",
        "32": "./assets/images/browser.png",
        "48": "./assets/images/browser.png",
        "64": "./assets/images/browser.png",
        "128": "./assets/images/browser.png"
    }
}
